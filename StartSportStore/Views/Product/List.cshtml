@model ProductListViewModel
@{
    ViewBag.Title = "List of Products";
    Layout = "_Layout";
}

    

@foreach (var p in Model.Products)
{
    @Html.Partial("product", p)
}
<div  page-model="@Model.pageInfo" page-action="List" class="btn-group pull-right btn-primary" page-classes-enabled="true" page-class="btn btn-primary" page-class-normal="btn-secondary" page-class-selected="btn btn-primary" >
</div>

<div page-model="@Model.pageInfo" page-action="List" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory"
     class="btn-group pull-right m-1">
</div>